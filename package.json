    "prepublishOnly": "yarn lint && yarn test",
    "build": "tsc",
    "format": "prettier -w ./src/**/*.{t,j}s",
    "lint": "eslint ./src --ext .js,.ts",
    "test": "jest src/"
  },
  "engines": {
    "node": "> 4"
  },
  "devDependencies": {
    "lock-treatment-tool": "^0.4.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.2",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "gitHead": "068789977eba80190531873dc290ea324a477f27"
