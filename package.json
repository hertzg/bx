{
  "name": "@hertzg/bx",
  "version": "0.0.1",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:hertzg/etekcity.git",
  "author": "George Hertz <georgedot@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "src/**/*",
    "!**/__tests__/*"
  ],
  "scripts": {
    "postinstall": "yarn fix-lockfiles",
    "fix-lockfiles": "locktt --registry=https://registry.npmjs.org -p",
